{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/users",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/test",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    
    {
      "src": "/birds",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/birds/about",
      "dest": "/index.js",
      "methods": ["GET"]
    },

    {
      "src": "/dictionary/api/v1/:words",
      "dest": "/index.js",
      "methods": ["GET"]
    },

    {
      "src": "/user",
      "dest": "/index.js",
      "methods": ["POST"]
    }
  ]
}
