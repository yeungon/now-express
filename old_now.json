{
  "version": 2,
  "builds": [{
    "src": "index.js",
    "use": "@vercel/node"
  }],
  "routes": [

    {
      "src": "/user",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/user/about",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/user/about/:id",
      "dest": "/index.js",
      "methods": ["GET"]
    },

    {
      "src": "/dictionary/api/v1/:words",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/task",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/task",
      "dest": "/index.js",
      "methods": ["POST"]
    }
  ]
}